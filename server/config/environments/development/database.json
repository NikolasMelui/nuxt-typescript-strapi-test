{
  "defaultConnection": "default",
  "connections": {
    "default": {
      "connector": "strapi-hook-mongoose",
      "settings": {
        "client": "mongo",
        "host": "${process.env.DB_HOST}",
        "database": "${process.env.DB_DATABASE}",
        "port": 27017,
        "username": "${process.env.DB_USER}",
        "password": "${process.env.DB_PASSWORD}",
        "srv": true
      },
      "options": {
        "authenticationDatabase": "admin",
        "ssl": true
      }
    }
  }
}
